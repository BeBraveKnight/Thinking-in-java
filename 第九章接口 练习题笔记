	public void f1(){
		System.out.println("f1()");
	}
	public void f2(){
		System.out.println("f2()");
	}
	public void f3(){
		System.out.println("f3()");
	}
	public static void main(String[]args){
		A174a a=new A174a();
		a.f1();
		a.f2();
		a.f3();
	}
}
